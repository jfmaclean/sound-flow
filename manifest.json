{
  "manifest_version": 2,

  "name": "Sound Flow",
  "description": "Enables seamless audio experiences across tabs",
  "version": "1.0",

  "permissions": [
    "tabs", "http://www.youtube.com/watch*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://www.youtube.com/watch*"],
      "js": ["jquery-2.0.3.min.js", "flow.js"],
      "run_at": "document_end"
    }
  ]
}